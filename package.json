{
  "name": "e-commerce-store",
  "version": "1.0.0",
  "description": "e-commerce application built with MERN stack",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\" ",
    "data:import":"node backend/seeder.js",
    "data:destroy":"node backend/seeder.js -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yixiongyuan/e-commerce-store.git"
  },
  "author": "zachary",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yixiongyuan/e-commerce-store/issues"
  },
  "homepage": "https://github.com/yixiongyuan/e-commerce-store#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "express": "^4.18.2",
    "mongoose": "^7.5.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "dotenv": "^16.3.1",
    "nodemon": "^3.0.1"
  }
}
